def add(x, y):
    """This function adds two numbers"""
    return x + y

def subtract(x, y):
    """This function subtracts two numbers"""
    return x - y

def multiply(x, y):
    """This function multiplies two numbers"""
    return x * y

def divide(x, y):
    """This function divides two numbers"""
    return x / y

# Example usage
num1 = 10
num2 = 5

print("Addition:", add(num1, num2)) # Output: Addition: 15
print("Subtraction:", subtract(num1, num2)) # Output: Subtraction: 5
print("Multiplication:", multiply(num1, num2)) # Output: Multiplication: 50
print("Division:", divide(num1, num2)) # Output: Division: 2.0
